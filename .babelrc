{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        },
        "debug": true
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-vue-jsx"
  ]
}
